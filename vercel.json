{
  "version": 2,
  "builds": [
    {
      "src": "parking/wsgi.py",
      "use": "@vercel/python",
      "buildCommand": "python manage.py collectstatic --noinput"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "parking/wsgi.py"
    }
  ]
}
